        "symfony/monolog-bundle": "^3.10",
        "vlucas/phpdotenv": "^5.6",
        "ext-pdo": "*",
        "firebase/php-jwt": "^6.11"
    },
    "authors": [
        {
            "name": "ismayil",
            "email": "me@ismayil.dev"
